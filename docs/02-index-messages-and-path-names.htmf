<pre><code filter="pbv" type="Msg">header {
    msg_id: '&lt;msg_id&gt;'
    msg_type: OPERATE
}
body {
    request {
        operate {
            command: 'Device.IP.Diagnostics.TraceRoute()'
            command_key: 'test64'
            send_resp: true
            input_args {
               key: 'Host'
               value: '&lt;remote host IP&gt;'
            }
            input_args {
               key: 'ProtocolVersion'
               value: 'Any'
            }
        }
    }
}</code></pre></li>
