    add {
      allow_partial: false
      create_objs {
        obj_path: 'Device.LocalAgent.Controller.[EndpointID=="&lt; EndpointID&gt;"&amp;&amp;Alias=="&lt;Alias if supported&gt;"].BootParameter.'
        param_settings {
