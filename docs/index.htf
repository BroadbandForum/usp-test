}
body {
  request {
    set {
      allow_partial: false
      update_objs {
        obj_path: 'Device.LocalAgent.Controller.&lt;instance identifier&gt;.'
        param_settings {
          param: 'ProvisioningCode'
          value: 'TestValue54'
          required: true
        }
      }
    }
  }
